{
    "manifests": {
        "friendsofphp/php-cs-fixer": {
            "manifest": {
                "copy-from-recipe": {
                    ".php-cs-fixer.dist.php": ".php-cs-fixer.dist.php"
                },
                "gitignore": [
                    "/.php-cs-fixer.cache"
                ]
            },
            "files": {
                ".php-cs-fixer.dist.php": {
                    "contents": [
                        "<?php\r",
                        "\r",
                        "$finder = (new PhpCsFixer\\Finder())\r",
                        "    ->in(__DIR__)\r",
                        "    ->exclude('var')\r",
                        ";\r",
                        "\r",
                        "return (new PhpCsFixer\\Config())\r",
                        "    ->setRules([\r",
                        "        '@Symfony' => true,\r",
                        "        '@PhpCsFixer' => true,\r",
                        "        '@PSR12' => true,\r",
                        "        '@PHP84Migration' => true,\r",
                        "    ])\r",
                        "    ->setFinder($finder)\r",
                        ";\r",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "50c8cae6a7a31998fd71118b97ce12a7655411b8"
        }
    }
}
