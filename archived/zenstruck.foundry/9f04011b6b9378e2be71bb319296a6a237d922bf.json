{
    "manifests": {
        "zenstruck/foundry": {
            "manifest": {
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "bundles": {
                    "Zenstruck\\Foundry\\ZenstruckFoundryBundle": [
                        "dev",
                        "test"
                    ]
                },
                "conflict": {
                    "symfony/framework-bundle": "<5.3"
                },
                "post-install-output": [
                    "  * You're ready to use zenstruck/foundry. Create your first factory with",
                    "    <info>bin/console make:factory</>.",
