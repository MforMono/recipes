                "features/demo.feature": {
                    "contents": [
                        "# This file contains a user story for demonstration only.\r",
                        "# Learn how to get started with Behat and BDD on Behat's website:\r",
                        "# http://behat.org/en/latest/quick_start.html\r",
                        "\r",
                        "Feature:\r",
                        "  In order to prove that the Behat Symfony extension is correctly installed\r",
                        "  As a user\r",
                        "  I want to have a demo scenario\r",
                        "\r",
                        "  Scenario: It receives a response from Symfony's kernel\r",
                        "    When a demo scenario sends a request to \"/\"\r",
                        "    Then the response should be received\r",
                        ""
                    ],
                    "executable": false
                },
                "tests/Behat/DemoContext.php": {
                    "contents": [
                        "<?php\r",
                        "\r",
                        "declare(strict_types=1);\r",
                        "\r",
                        "namespace App\\Tests\\Behat;\r",
                        "\r",
                        "use Behat\\Behat\\Context\\Context;\r",
                        "use Symfony\\Component\\HttpFoundation\\Request;\r",
                        "use Symfony\\Component\\HttpFoundation\\Response;\r",
                        "use Symfony\\Component\\HttpKernel\\KernelInterface;\r",
                        "\r",
                        "/**\r",
                        " * This context class contains the definitions of the steps used by the demo\r",
                        " * feature file. Learn how to get started with Behat and BDD on Behat's website.\r",
                        " *\r",
                        " * @see http://behat.org/en/latest/quick_start.html\r",
                        " */\r",
                        "final class DemoContext implements Context\r",
                        "{\r",
                        "    /** @var KernelInterface */\r",
                        "    private $kernel;\r",
                        "\r",
                        "    /** @var Response|null */\r",
                        "    private $response;\r",
                        "\r",
                        "    public function __construct(KernelInterface $kernel)\r",
                        "    {\r",
                        "        $this->kernel = $kernel;\r",
                        "    }\r",
                        "\r",
                        "    /**\r",
                        "     * @When a demo scenario sends a request to :path\r",
                        "     */\r",
                        "    public function aDemoScenarioSendsARequestTo(string $path): void\r",
                        "    {\r",
                        "        $this->response = $this->kernel->handle(Request::create($path, 'GET'));\r",
                        "    }\r",
                        "\r",
                        "    /**\r",
                        "     * @Then the response should be received\r",
                        "     */\r",
                        "    public function theResponseShouldBeReceived(): void\r",
                        "    {\r",
                        "        if ($this->response === null) {\r",
                        "            throw new \\RuntimeException('No response received');\r",
                        "        }\r",
                        "    }\r",
                        "}\r",
                        ""
                    ],
                    "executable": false
                }
            },
