{
    "manifests": {
        "robertfausk/behat-panther-extension": {
            "manifest": {
                "add-lines": [
                    {
                        "file": "behat.dist.php",
                        "content": "    ->withExtension(new Extension(Robertfausk\\Behat\\PantherExtension\\ServiceContainer\\PantherExtension::class))",
                        "position": "after_target",
                        "target": "# Extensions",
                        "warn_if_missing": true
                    },
                    {
                        "file": "behat.dist.php",
                        "content": "        'javascript_session' => 'panther',",
                        "position": "after_target",
                        "target": "'default_session' => 'symfony',",
                        "warn_if_missing": true
                    },
                    {
                        "file": "behat.dist.php",
                        "content": "            'panther' => [\n                'panther' => [\n                    'options' => [\n                        'browser' => 'chrome',\n                        'webServerDir' => '%paths.base%/public',\n                        'external_base_uri' => 'https://127.0.0.1:8080',\n                    ],\n                    'kernel_options' => [\n                        'APP_ENV' => 'test',\n                        'APP_DEBUG' => false,\n                    ],\n                    'manager_options' => [\n                        'connection_timeout_in_ms' => 5000,\n                        'request_timeout_in_ms' => 120000,\n                        'capabilities' => [\n                            'browserName' => 'chrome',\n                            'browser' => 'chrome',\n                            'acceptSslCerts' => true,\n                            'acceptInsecureCerts' => true,\n                            'unexpectedAlertBehaviour' => 'accept',\n                        ],\n                        'extra_capabilities' => [\n                            'chromeOptions' => [\n                                'args' => ['\n                                    '--window-size=2880,1800',\n                                    '--headless',\n                                    'start-fullscreen',\n                                    'start-maximized',\n                                    'no-sandbox',\n                                ],\n                            ],\n                        ],\n                    ],\n                ],\n            ],",
                        "position": "after_target",
                        "target": "# Sessions",
                        "warn_if_missing": true
                    }
                ]
            },
            "files": [],
            "ref": "69e8bc03cc7ca7c80029f4f6e30ab28dc90b2af8"
        }
    }
}
